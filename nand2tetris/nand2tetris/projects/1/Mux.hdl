/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	Nand(a=sel, b=a, out=ourXor1);
	Not(in=sel, out=outNotSel);
	Xor(a=outNotSel, b=b, out=outXor2);
}